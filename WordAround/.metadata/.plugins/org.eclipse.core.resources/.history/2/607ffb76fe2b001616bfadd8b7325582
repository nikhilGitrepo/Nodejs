var connect = require('connect');
var http = require('http');

var app = connect();

app.use(function(req, res, next) {
	response.writeHead(200,{"Content-Type":"text/plain"});
	response.write('Error 404: Page not found!');
	response.end();
})

http.createServer(app).listen(8888);
console.log('niki;');